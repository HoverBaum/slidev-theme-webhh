{
  "name": "slidev-theme-webhh",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.49.0-beta.4",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4",
    "qrcode.vue": "^3.4.1"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.0-beta.4",
    "playwright-chromium": "^1.43.1"
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "aspectRatio": "16/9",
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}
